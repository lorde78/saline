{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "nodemon --ignore node_modules ./bin/www",
    "dev": "nodemon --config nodemon.json src/index.ts",
    "dev:debug": "nodemon --config nodemon.json --inspect-brk src/index.ts",
    "restart": "docker-compose down && docker-compose up -d",
    "kill": "docker system prune -a",
    "tsw": "tsc --watch",
    "prisma-install": "npm install @prisma/client",
    "prs-run": "cd node_modules/.prisma/client; npx prisma generate",
    "prs-migrate": "npx prisma migrate dev --schema=./prisma/shema.prisma",
    "faker-data": "npx prisma db seed",
    "debug-routes": "DEBUG=express:router node app.js"
  },
  "nodemonConfig": {
    "legacyWatch": true,
    "ext": "js,json,mjs,pug,ts"
  },
  "dependencies": {
    "@faker-js/faker": "^8.0.2",
    "@prisma/client": "^5.0.0",
    "bcrypt": "^5.1.0",
    "clear-module": "^4.1.2",
    "cookie-parser": "~1.4.4",
    "crypto": "^1.0.1",
    "crypto-js": "^4.1.1",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-errors": "~1.6.3",
    "i": "^0.3.7",
    "jsonwebtoken": "^9.0.1",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1",
    "npm": "^9.8.0",
    "prisma": "^5.0.0",
    "pug": "^2.0.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.1",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
